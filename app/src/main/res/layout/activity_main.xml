<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <include
        android:layout_width="match_parent"
        android:layout_height="175dp"
        layout="@layout/main_window"
        android:id="@+id/main_window"/>

    <include
        android:id="@+id/equalizer_window"
        layout="@layout/equalizer_window"
        app:layout_constraintTop_toTopOf="@id/main_window"
        app:layout_editor_absoluteY="176dp" />

    <include
        android:id="@+id/playlist_window"
        layout="@layout/playlist_window"
        app:layout_constraintTop_toBottomOf="@id/equalizer_window"
        app:layout_constraintBottom_toBottomOf="parent"/>


</androidx.constraintlayout.widget.ConstraintLayout>